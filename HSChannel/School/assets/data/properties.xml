<properties>

    <features>


        <feature
            name="Cache"
            value="io.dcloud.feature.pdr.CoreCacheFeatureImpl" >
        </feature>

        <feature
            name="Camera"
            value="io.dcloud.js.camera.CameraFeatureImpl" >
        </feature>

        <feature
            name="Device"
            value="io.dcloud.feature.device.DeviceFeatureImpl" >
        </feature>


        <feature
            name="File"
            value="io.dcloud.js.file.FileFeatureImpl" >
        </feature>

        <feature
            name="Gallery"
            value="io.dcloud.js.gallery.GalleryFeatureImpl" >
        </feature>

        <feature
            name="Geolocation"
            value="io.dcloud.js.geolocation.GeolocationFeatureImpl" />

        <feature
            name="Invocation"
            value="io.dcloud.invocation.Invocation" >
        </feature>

        <feature
            name="Messaging"
            value="io.dcloud.adapter.messaging.MessagingPluginImpl" >
        </feature>

        <feature
            name="Orientation"
            value="io.dcloud.feature.sensor.OrientationFeatureImpl" />

        <feature
            name="Proximity"
            value="io.dcloud.feature.sensor.ProximityFeatureImpl" >
        </feature>

        <feature
            name="Storage"
            value="io.dcloud.feature.pdr.NStorageFeatureImpl" >
        </feature>

        <feature
            name="Navigator"
            value="io.dcloud.feature.ui.navigator.NavigatorUIFeatureImpl" >
        </feature>

        <feature
            name="NativeObj"
            value="io.dcloud.feature.nativeObj.FeatureImpl" />

        <feature
            name="NativeUI"
            value="io.dcloud.feature.ui.nativeui.NativeUIFeatureImpl" >
        </feature>

        <feature
            name="UI"
            value="io.dcloud.feature.ui.UIFeatureImpl" >

            <module
                name="Navigator"
                value="io.dcloud.feature.ui.NavView" />
        </feature>

        <feature
            name="Runtime"
            value="io.dcloud.feature.pdr.RuntimeFeatureImpl" >
        </feature>

        <feature
            name="XMLHttpRequest"
            value="io.dcloud.net.XMLHttpRequestFeature" >
        </feature>


        <feature
            name="OAuth"
            value="io.dcloud.feature.oauth.OAuthFeatureImpl" >
        </feature>






        <feature
            name="plugintest"
            value="com.example.H5PlusPlugin.PGPlugintest" />
    </features>

    <services>

        <!-- 如果扩展插件需要在应用启动时初始化可以在这里添加一个service -->
        <!-- service name="plugintest" value="com.example.H5PlusPlugin.PGPlugintest" / -->


    </services>

</properties>